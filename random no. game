#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    // Seed the random number generator
    srand(time(0));

    // Generate a random number between 1 and 100
    int random_number = (rand() % 100) + 1;
    int no_of_guesses = 0,gussed;

    do
    {
        printf("guess the no.:");
        scanf("%d",&gussed);
        if (gussed>random_number)
        {
            printf("lower no, plzz\n");
        }
        else
        printf("higher no. plzz\n");
        
        no_of_guesses++;
    } while (gussed!=random_number);
    printf("u guessed the no. in %d guesses",no_of_guesses);
    

    // // Print the random number
    // printf("Random number between 1 and 100: %d\n", random_number);

    return 0;
}
